<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="200dp"
    android:orientation="vertical"
    android:background="#f83139">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:orientation="horizontal">
        <ImageView
            android:id="@+id/ic_fanhui"
            android:layout_marginLeft="10dp"
            android:layout_gravity="center"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_back_white"/>
        <TextView
            android:gravity="center"
            android:textSize="20sp"
            android:layout_gravity="center"
            android:textColor="@color/white"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="云链健康"/>



    </LinearLayout>


<TextView
    android:layout_marginLeft="8dp"
    android:layout_marginTop="34dp"
    android:textSize="14sp"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="达事币："
    android:textColor="#fff"
   />
    <TextView
        android:id="@+id/shejiao_lian"
        android:layout_marginLeft="8dp"
        android:textStyle="bold"
        android:textSize="31sp"
        android:layout_marginTop="31dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:textColor="@color/white"/>
</LinearLayout>


    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        >
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">




    <LinearLayout style="@style/LinearLayout_Divider"
        android:layout_marginTop="15dp">
        <RelativeLayout
            android:id="@+id/my_personmoeney"
            style="@style/RelativeLayout_Default">
            <ImageView
                android:layout_centerVertical="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/my_personmoeney_im"
                android:src="@drawable/et_my_shejiaoyue"/>
            <TextView
                android:layout_marginLeft="10dp"

                android:layout_toRightOf="@+id/my_personmoeney_im"
                style="@style/TextView_Style_Title"
                android:layout_centerVertical="true"
                android:text="我的余额" />

            <TextView
                android:gravity="center"
                android:layout_marginRight="5dp"
                android:id="@+id/shejiao_money"
                android:layout_toLeftOf="@+id/im_moeney_get"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""/>
            <ImageView
                android:id="@+id/im_moeney_get"
                style="@style/ImageView_RelativeLayout_Right" />
        </RelativeLayout>

        <RelativeLayout
             android:id="@+id/my_fabu"
            style="@style/RelativeLayout_Default">
         <ImageView
            android:layout_centerVertical="true"
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:id="@+id/zc_fabu_im"
             android:src="@drawable/et_fabu"/>
            <TextView
                android:layout_marginLeft="10dp"
                android:id="@+id/zc_fabu"
                android:layout_toRightOf="@+id/zc_fabu_im"
                style="@style/TextView_Style_Title"
                android:layout_centerVertical="true"
                android:text="我要发布" />
            <ImageView
                style="@style/ImageView_RelativeLayout_Right" />
        </RelativeLayout>

        <RelativeLayout
             android:id="@+id/my_fabu_jilu"
            style="@style/RelativeLayout_Default">
            <ImageView
                android:layout_centerVertical="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/zc_dating_im"
                android:src="@drawable/et_fabu_jilu"/>
            <TextView
                android:layout_marginLeft="10dp"
                android:layout_toRightOf="@+id/zc_dating_im"
                style="@style/TextView_Style_Title"
                android:text="发布记录" />

            <ImageView

                style="@style/ImageView_RelativeLayout_Right" />
        </RelativeLayout>

        <RelativeLayout
              android:id="@+id/jiaoyi_dating"
            style="@style/RelativeLayout_Default">
            <ImageView
                android:layout_centerVertical="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/zc_jiaoyi_im"
                android:src="@drawable/et_jiaoyi"/>
            <TextView
                android:layout_marginLeft="10dp"
                android:layout_toRightOf="@+id/zc_jiaoyi_im"
                style="@style/TextView_Style_Title"
                android:text="交易大厅" />

            <TextView
                style="@style/TextView_Style_More"
                android:text="" />
        </RelativeLayout>

        <RelativeLayout
         android:id="@+id/jiaoyijilu"
            style="@style/RelativeLayout_Default">
            <ImageView
                android:layout_centerVertical="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/zc_jiaoyijilu_im"
                android:src="@drawable/et_jilu"/>
            <TextView
                android:layout_marginLeft="10dp"
                android:layout_toRightOf="@+id/zc_jiaoyijilu_im"
                style="@style/TextView_Style_Title"
                android:text="交易记录" />

            <TextView
                style="@style/TextView_Style_More"
                android:text="" />
        </RelativeLayout>
        <RelativeLayout
           android:id="@+id/my_zichan_zhihuan"
            style="@style/RelativeLayout_Default">
            <ImageView
                android:layout_centerVertical="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/zc_zhihuan_im"
                android:src="@drawable/et_zhihuanjilu"/>
            <TextView
                android:layout_marginLeft="10dp"
               android:layout_toRightOf="@+id/zc_zhihuan_im"
                style="@style/TextView_Style_Title"
                android:text="置换记录" />

            <TextView
                style="@style/TextView_Style_More"
                android:text="" />
        </RelativeLayout>
        <RelativeLayout
              android:id="@+id/shejiaolian_mingxi"
            style="@style/RelativeLayout_Default">
            <ImageView
                android:layout_centerVertical="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/zc_zichan_im"
                android:src="@drawable/et_zichuan"/>
            <TextView
                android:layout_marginLeft="10dp"
                android:layout_toRightOf="@+id/zc_zichan_im"
                style="@style/TextView_Style_Title"
                android:text="达事币明细" />

            <TextView
                style="@style/TextView_Style_More"
                android:text="" />
        </RelativeLayout>




</LinearLayout>

        </LinearLayout>
    </ScrollView>


</LinearLayout>
