<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/RelativeLayout_Default">

    <TextView
        android:text="da2111"
        android:id="@+id/tv_username"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <TextView
        android:text="da2111"
        android:id="@+id/username"
        android:layout_marginLeft="@dimen/margin_default"
        android:layout_marginStart="@dimen/margin_default"
        android:layout_alignBottom="@+id/tv_username"
        android:layout_toRightOf="@+id/tv_username"
        android:layout_toEndOf="@+id/tv_username"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <TextView
        android:text="da2111"
        android:id="@+id/tv_unit_price"
        android:layout_marginTop="@dimen/margin_10"
        android:layout_below="@+id/username"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <TextView
        android:text="da2111"
        android:id="@+id/unit_price"
        android:layout_marginLeft="@dimen/margin_default"
        android:layout_marginStart="@dimen/margin_default"
        android:layout_alignBottom="@+id/tv_unit_price"
        android:layout_toRightOf="@+id/tv_unit_price"
        android:layout_toEndOf="@+id/tv_unit_price"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <TextView
        android:text="da2111"
        android:id="@+id/tv_numbers"
        android:layout_marginTop="@dimen/margin_10"
        android:layout_below="@+id/tv_unit_price"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <TextView
        android:text="da2111"
        android:id="@+id/numbers"
        android:layout_marginLeft="@dimen/margin_default"
        android:layout_marginStart="@dimen/margin_default"
        android:layout_alignBottom="@+id/tv_numbers"
        android:layout_toRightOf="@+id/tv_numbers"
        android:layout_toEndOf="@+id/tv_numbers"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <TextView
        android:text="da2111"
        android:id="@+id/tv_create_time"
        android:layout_marginTop="@dimen/margin_10"
        android:layout_below="@+id/tv_numbers"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <TextView
        android:text="da2111"
        android:id="@+id/create_time"
        android:layout_marginLeft="@dimen/margin_default"
        android:layout_marginStart="@dimen/margin_default"
        android:layout_alignBottom="@+id/tv_create_time"
        android:layout_toRightOf="@+id/tv_create_time"
        android:layout_toEndOf="@+id/tv_create_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
</RelativeLayout>